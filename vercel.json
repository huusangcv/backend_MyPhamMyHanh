{ 
    "version": 2,
    "builds":[
        {
        "src": "src/index.ts",
        "use": "@vercel/node"
        }
    ],
     "routes": [
        {
            "src": "/index/(.*)",
            "dest": "src/index.ts"
        },
          {
            "src": "/(.*)",
            "dest": "/"
        }
    ]  
}